    "139a823f-816a-453f-b7cc-090192aba72d": {
      "type": "custom-rich-text",
      "blocks": {
        "template--16034682503308__139a823f-816a-453f-b7cc-090192aba72d-heading-1": {
          "type": "heading",
          "settings": {
            "text": "The Price of This Trip Includes",
            "size": "h3"
